@model SFA.DAS.ApplyService.Web.ViewModels.Roatp.UkprnSearchResultsViewModel

@{
    ViewBag.Title = "Charity registration number not found";
    Layout = "_Layout";
}

@{ await Html.RenderPartialAsync("~/Views/Roatp/PreambleHeader.cshtml"); }

<form asp-controller="RoatpApplicationPreamble" asp-action="EnterApplicationUkprn" method="get" name="backForm">
    <input type="hidden" asp-for="UKPRN" />
    <input type="hidden" asp-for="ApplicationRouteId" />
    <a href="JavaScript:document.backForm.submit();" class="govuk-back-link">Back</a>
</form>

<main class="govuk-main-wrapper " id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-xl">Charity registration number @Model.CharityNumber not found</h1>
            <p class="govuk-body">This is because it either:</p>
            <ul class="govuk-list govuk-list--bullet">
                <li>does not exist</li>
                <li>is deregistered on the Charity Commission</li>
            </ul>
            <p class="govuk-body">
                If you think this is wrong, contact the UKRLP helpdesk on 0345 202 1600 or <a class="govuk-link" href="mailto:helpdesk@ukrlp.co.uk">helpdesk@ukrlp.co.uk</a>. You can also <a href="https://www.gov.uk/government/organisations/charity-commission/about-our-services" target="_blank" class="govuk-link">contact the Charity Commission (opens in a new window or tab).</a>
            </p>
        </div>
    </div>
</main>