@model SFA.DAS.ApplyService.Web.ViewModels.Roatp.SearchByUkprnViewModel

@{
    ViewBag.Title = "You have chosen not to apply to join the RoATP";
    Layout = "_Layout";
}

@{ await Html.RenderPartialAsync("~/Views/Roatp/PreambleHeader.cshtml"); }

<div class="grid-row">
    <form asp-controller="RoatpApplicationPreamble" asp-action="IneligibleNonLevy" method="get" name="backForm">
        <a href="JavaScript:document.backForm.submit();" class="govuk-back-link">Back</a>
    </form>
</div>

<main class="govuk-main-wrapper " id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-l">You have chosen not to apply to join the RoATP</h1>
            <p class="govuk-body"><a href="https://www.gov.uk" class="govuk-link">Back to GOV.UK</a></p>
        </div>
    </div>
</main>