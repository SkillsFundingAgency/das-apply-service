@model SFA.DAS.ApplyService.Web.ViewModels.Roatp.ApplicationSummaryViewModel

@{
    ViewBag.Title = "Application withdrawn";
    Layout = "_Layout";
}

@section Navigation
{
    <partial name="~/Views/Application/Navigation/_ApplicationHeaderReadOnly.cshtml" />
}

<main class="govuk-main-wrapper " id="main-content" role="main">

    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">

            <div class="govuk-panel app-panel--muted">
                <h1 class="govuk-panel__title">
                    Application withdrawn
                </h1>
            </div>

            <partial name="~/Views/Roatp/_ApplicationDetailsTable.cshtml" model="@Model" />

            <h1 class="govuk-heading-m">Why your application was withdrawn</h1>
            <p class="govuk-body das-multiline-text">
                @Model.GatewayExternalComments
            </p>
        </div>
    </div>

    <partial name="~/Views/Roatp/_AppealComplaintContact.cshtml"  model="@Model" />
</main>