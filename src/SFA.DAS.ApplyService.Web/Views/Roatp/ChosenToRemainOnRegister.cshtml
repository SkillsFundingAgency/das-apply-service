@model SFA.DAS.ApplyService.Web.ViewModels.Roatp.ChangeProviderRouteViewModel

@{
    ViewBag.Title = "You have chosen to stay on the RoATP";
    Layout = "_Layout";
}

@{ await Html.RenderPartialAsync("~/Views/Roatp/PreambleHeader.cshtml"); }

<div class="grid-row">
    <form asp-controller="RoatpApplicationPreamble" asp-action="ProviderAlreadyOnRegister" method="get" name="backForm">
        <a href="JavaScript:document.backForm.submit();" class="govuk-back-link">Back</a>
    </form>
</div>

<div class="govuk-width-container">

    <main class="govuk-main-wrapper " id="main-content" role="main">

        <div class="govuk-grid-row">
            <div class="govuk-grid-column-two-thirds">
                <h1 class="govuk-heading-l">You have chosen to stay on the RoATP as @Model.IndefiniteArticle @Model.CurrentProviderType.RouteName</h1>
                <p class="govuk-body"><a href="https://www.gov.uk">Back to GOV.UK</a></p>
            </div>
        </div>
    </main>

</div>